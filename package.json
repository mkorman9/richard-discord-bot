  "license": "MIT",
  "main": "dist",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "compile": "tsc -p tsconfig.json",
    "build": "npm-run-all clean lint compile",
    "serve": "TZ=UTC NODE_ENV=development node ./dist",
    "start": "tsc-watch --onSuccess \"npm-run-all lint serve\""
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "axios": "^0.21.2",
    "discord.js": "^13.1.0",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-schedule": "^2.0.0",
    "splitargs2": "^0.1.3",
    "sqlite3": "^5.0.2",
    "twing": "^5.0.2",
    "winston": "^3.3.3",
    "yaml": "^1.10.2",
    "youtube-dl-exec": "^1.2.5",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "@types/shelljs": "^0.8.9",
    "@types/sqlite3": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
